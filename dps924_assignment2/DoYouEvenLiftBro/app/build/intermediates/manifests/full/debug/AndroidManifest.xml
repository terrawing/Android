<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.student.doyouevenliftbro"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.student.doyouevenliftbro.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.student.doyouevenliftbro.ViewAllExercisesActivity"
            android:label="@string/title_activity_view_all_exercises"
            android:parentActivityName="com.example.student.doyouevenliftbro.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.ViewExercisesByMuscleActivity"
            android:label="@string/title_activity_view_exercises_by_muscle"
            android:parentActivityName="com.example.student.doyouevenliftbro.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.DisplayAndDoExercisesActivity"
            android:label="@string/title_activity_display_and_do_exercises"
            android:parentActivityName="com.example.student.doyouevenliftbro.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.OptionsActivity"
            android:label="@string/title_activity_options"
            android:parentActivityName="com.example.student.doyouevenliftbro.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.ViewAllExercise_ListView"
            android:label="@string/title_activity_view_all_exercise__list_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.AllExerciseDisplayActivity"
            android:label="@string/title_activity_exercise"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.ExercisesByMuscleActivity"
            android:label="@string/title_activity_exercises_by_muscle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.AddExerciseActivity"
            android:label="@string/title_activity_add_exercise"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.ExercisesPerDayActivity"
            android:label="@string/title_activity_exercises_per_day"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.Draggable_ListView"
            android:label="@string/title_activity_draggable__list_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.DoExerciseDisplayActivity"
            android:label="@string/title_activity_do_exercise_display"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.AddStatsActivity"
            android:label="@string/title_activity_add_stats"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.student.doyouevenliftbro.Custom_ListView"
            android:label="@string/title_activity_custom__list_view"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>